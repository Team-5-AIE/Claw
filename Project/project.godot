run/max_fps=144
config/icon="res://icon.svg"
boot_splash/assembly_name="MoverInThePlane_Prototype_demo"

[autoload]

Global="*res://Scripts/Global.gd"

[debug]

settings/canvas_textures/default_texture_filter=3

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/scale=2.0
window/stretch/scale_mode="integer"
window/screen_space_roughness_limiter/enabled=true

[dotnet]

project/assembly_name="MoverInThePlane_Prototype_demo"

[editor]

movie_writer/fps=160

[file_customization]

folder_colors={
"res://Objects/": "green",
"res://SWPlatformerPackage/": "purple",
"res://Scripts/": "red",
"res://Sprites/": "blue"
}

